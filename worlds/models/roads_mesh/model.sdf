      <visual name="vis">
        <geometry>
          <mesh>
            <uri>model://roads_mesh/meshes/roads_mesh.obj</uri>
          </mesh>
        </geometry>
        <material>
          <diffuse>0.4 0.4 0.4 1</diffuse>
          <ambient>0.3 0.3 0.3 1</ambient>
          <pbr>
            <metal>
              <albedo_map>model://roads_mesh/materials/textures/asphalt_basecolor.png</albedo_map>
