              <roughness_map>model://roads_mesh/materials/textures/asphalt_roughness.png</roughness_map>
              <metalness>0.0</metalness>
              <roughness>0.8</roughness>
            </metal>
          </pbr>
        </material>
      </visual>
      <collision name="col">
        <geometry>
          <mesh>
            <uri>model://roads_mesh/meshes/roads_mesh.obj</uri>
          </mesh>
        </geometry>
      </collision>
    </link>
